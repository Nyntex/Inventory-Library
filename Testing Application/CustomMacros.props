<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <ThirdPartyDir>$(ProjectDir)ThirdParty</ThirdPartyDir>
    <ThirdPartyLibDir>$(ThirdPartyDir)\Lib\$(Configuration)\$(Platform)</ThirdPartyLibDir>
    <ThirdPartyHeaderDir>$(ThirdPartyDir)\Header</ThirdPartyHeaderDir>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>ThirdParty</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="ThirdPartyDir">
      <Value>$(ThirdPartyDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="ThirdPartyLibDir">
      <Value>$(ThirdPartyLibDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="ThirdPartyHeaderDir">
      <Value>$(ThirdPartyHeaderDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>